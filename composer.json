{
    "name": "sandorian/moneybird-api-php",
    "description": "Moneybird API client for PHP",
    "keywords": [
        "moneybird",
        "bookkeeping",
        "php",
        "sandorian",
        "moneybird-api-php"
    ],
    "homepage": "https://github.com/sandorian/moneybird-api-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Sander van Hooft (Sandorian.com)",
            "homepage": "https://sandorian.com",
            "email": "sander@sandorian.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.9",
        "saloonphp/pagination-plugin": "^2.2",
        "saloonphp/saloon": "^3.10"
    },
    "require-dev": {
        "laravel/pint": "^1.17",
        "phpunit/phpunit": "^11.3",
        "symfony/var-dumper": "^7.1"
    },
    "autoload": {
        "files": [
            "tests/helpers.php"
        ],
        "psr-4": {
            "Sandorian\\Moneybird\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sandorian\\Moneybird\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
